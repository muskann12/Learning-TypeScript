//string
let message :string ="hello its me muskan here"
console.log(message)

//number
let numA :number = 20
let numB :number = 50
console.log(numA + numB)

//boolean
let isPaid :boolean = false
console.log(isPaid)

//Bigint
let x :bigint = 92345897065740n
let y :bigint = 901235547088640n
let sum :bigint = x+y
console.log("sum of x and y", sum)


//null
let or = null
console.log(or)


